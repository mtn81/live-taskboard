{
  "bundles": {
    "dist/app-build": {
      "includes": [
        "[**/*]",
        "*.html!text",
        "**/*.css!text"
      ],
      "options": {
        "inject": true,
        "minify": false
      }
    },
    "dist/aurelia": {
      "includes": [
        "aurelia-framework",
        "aurelia-bootstrapper",
        "aurelia-fetch-client",
        "aurelia-router",
        "aurelia-animator-css",
        "aurelia-templating-binding",
        "aurelia-templating-resources",
        "aurelia-templating-router",
        "aurelia-loader-default",
        "aurelia-history-browser",
        "aurelia-logging-console",
        "bootstrap",
        "bootstrap/css/bootstrap.css!text",
        
        "aurelia-http-client",
        "stomp-websocket",
        "bootbox",
        "underscore",
        "components/jqueryui",
        "jquery",
        "css",
        "bootstrap-select/js/bootstrap-select",
        "bootstrap-select/css/bootstrap-select.css!text"
        
      ],
      "options": {
        "inject": true,
        "minify": false,
        "rev": true
      }
    }
  }
}
