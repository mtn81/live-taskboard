server:
  context-path: /task-manage
  port: 8080

spring:
  profiles:
    active: dev
  datasource:
    url: "jdbc:postgresql://{{tm_db_host | consul}}:5432/task-manage"
    username: app
    password: pass
    driver-class-name: org.postgresql.Driver

rabbitmq:
  host: "{{mq_host | consul}}"
  username: app
  password: pass
  exchange: task-manage

cooperation:
  auth-access-url: "http://{{web_host | consul}}/api/auth-access"

domain:
  member-auth-expiration-minutes: 30

logging:
  file: /var/log/app.log
