server:
  context-path: /widget-store
  port: 8080

spring:
  profiles:
    active: dev
  datasource:
    url: jdbc:postgresql://{{ws_db_host | consul}}:5432/widget-store
    username: app
    password: pass
    driver-class-name: org.postgresql.Driver

rabbitmq:
  host: {{mq_host | consul}}
  username: app
  password: pass
  exchange: widget-store

cooperation:
  task-manage-url: http://{{web_host}}/api/task-manage

logging:
  file: /var/log/app.log
